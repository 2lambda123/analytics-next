{
  "name": "@segment/analytics-node",
  "version": "1.0.0-beta.22",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "!**/__tests__/**",
    "!*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "yarn jest",
    "lint": "yarn concurrently 'yarn:eslint .' 'yarn:tsc --noEmit'",
    "build": "rm -rf dist && yarn tsup src/index.ts --format esm,cjs --dts --splitting --clean --treeshake",
    "version": "sh scripts/version.sh",
    "watch": "yarn build:esm --watch",
    "watch:test": "yarn test --watch",
    "tsc": "yarn run -T tsc",
    "eslint": "yarn run -T eslint",
    "concurrently": "yarn run -T concurrently",
    "jest": "yarn run -T jest",
    "tsup": "yarn run -T tsup",
    "publish-prerelease": "sh scripts/prerelease.sh"
  },
  "dependencies": {
    "@segment/analytics-core": "1.2.2",
    "buffer": "^6.0.3",
    "node-fetch": "^2.6.7",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@internal/config": "0.0.0",
    "@types/node": "^16",
    "@types/uuid": "^9"
  },
  "packageManager": "yarn@3.2.1"
}
